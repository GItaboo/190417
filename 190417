#Bubble sort
'''
a = [5,4,3,1,2]


for j in range(len(a)) :

    for i in range(len(a)-1) :

        if(a[i] > a[i+1]) :

            a[i], a[i+1] = a[i+1], a[i]

print(a)
'''

a = [5,4,3,1,2]

for k in range(len(a)) :
    temp = k
    for i in range(k,len(a)) :
        if(a[i] < a[temp]) :
            temp = i
    a[k],a[temp] = a[temp],a[k]

print(a)


